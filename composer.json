{
    "name": "fawest/laravel-meta-tags",
    "description": "Meta Tags for Laravel 7 and up",
    "keywords": ["laravel", "tags", "seo", "meta"],
    "license": "MIT",
    "authors": [
        {
            "name": "lor08",
            "email": "spike.09@ya.ru"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Fawest\\MetaTags\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fawest\\MetaTags\\MetaTagsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
